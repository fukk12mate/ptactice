using System;

class Program
{
  public static void Main (string[] args) 
  {
    Console.Write("Введите целое положительное число n: ");
    int n = int.Parse(Console.ReadLine());

    long operation = 1;

    for (int i = 3; i <= n; i += 3)
    {
      operation *= i;
    }

    Console.WriteLine($"Произведение натуральных чисел, кратных трём и не превышающих {n}, равно: {operation}");
  }
}
