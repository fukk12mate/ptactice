using System;

class Program 
{
  public static void Main (string[] args) 
  {
    double x = 0, y = 0;
    Console.WriteLine ("Введите значение x:");
    x = Convert.ToDouble(Console.ReadLine());

    Console.WriteLine ("Введите значение y:");
    y = Convert.ToDouble(Console.ReadLine());

    double x1 = 0, y1 = 2;
    double x2 = -2, y2 = -3;
    double x3 = 2, y3 = -3;
    double s = 0, s1 = 0, s2 = 0, s3 = 0;


    s = Math.Abs((x1 * y2 + x2 * y3 + x3 * y1 - y1 * x2 - y2 * x3 - y3 * x1) / 2);
    s1 = Math.Abs((x * y2 + x2 * y3 + x3 * y - y * x2 - y2 * x3 - y3 * x) / 2);
    s2 = Math.Abs((x1 * y + x * y3 + x3 * y1 - y1 * x - y * x3 - y3 * x1) / 2);
    s3 = Math.Abs((x1 * y2 + x2 * y + x * y1 - y1 * x2 - y2 * x - y * x1) / 2);


    if (s == s1 + s2 + s3)
    {
            Console.WriteLine("Точка принадлежит области треугольника");
    }
    else 
    {
            Console.WriteLine("Точка не принадлежит области треугольника");
    }
  }
}